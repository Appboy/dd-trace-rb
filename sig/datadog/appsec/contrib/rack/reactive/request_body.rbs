module Datadog
  module AppSec
    module Contrib
      module Rack
        module Reactive
          # Dispatch data from a Rack request to the WAF context
          module RequestBody
            def self.publish: (untyped op, untyped request) -> untyped

            def self.subscribe: (untyped op, untyped waf_context) { (untyped) -> untyped } -> untyped
          end
        end
      end
    end
  end
end
